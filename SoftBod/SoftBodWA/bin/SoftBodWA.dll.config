<?xml version="1.0"?>
<!--
  Para obtener más información sobre cómo configurar la aplicación ASP.NET, visite
  https://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="VentaPortBinding" />
				<binding name="ProductoPortBinding" maxBufferSize="2147483647" maxReceivedMessageSize="2147483647" />
				<binding name="UsuarioPortBinding" />
				<binding name="CategoriaPortBinding" />
				<binding name="ClienteAlFiadoPortBinding" />
				<binding name="DetalleDevolucionPortBinding" />
				<binding name="DetalleVentaPortBinding" />
				<binding name="DevolucionPortBinding" />
				<binding name="HistorialOperacionesPortBinding" />
				<binding name="RazonDevolucionPortBinding" />
				<binding name="RegistroPagoFiadoPortBinding" />
				<binding name="VentaAlFiadoPortBinding" />
			</basicHttpBinding>
		</bindings>
		<client>
			<endpoint address="http://localhost:8080/SoftBodWS/Venta" binding="basicHttpBinding"
                bindingConfiguration="VentaPortBinding" contract="SoftWSVenta.Venta"
                name="VentaPort" />
			<endpoint address="http://localhost:8080/SoftBodWS/Producto"
                binding="basicHttpBinding" bindingConfiguration="ProductoPortBinding"
                contract="SoftWSProducto.Producto" name="ProductoPort" />
			<endpoint address="http://localhost:8080/SoftBodWS/Usuario" binding="basicHttpBinding"
                bindingConfiguration="UsuarioPortBinding" contract="SoftWSUsuario.Usuario"
                name="UsuarioPort" />
			<endpoint address="http://localhost:8080/SoftBodWS/Categoria"
                binding="basicHttpBinding" bindingConfiguration="CategoriaPortBinding"
                contract="SoftWSCategoria.Categoria" name="CategoriaPort" />
			<endpoint address="http://localhost:8080/SoftBodWS/ClienteAlFiado"
                binding="basicHttpBinding" bindingConfiguration="ClienteAlFiadoPortBinding"
                contract="SoftWSClienteAlFiado.ClienteAlFiado" name="ClienteAlFiadoPort" />
			<endpoint address="http://localhost:8080/SoftBodWS/DetalleDevolucion"
                binding="basicHttpBinding" bindingConfiguration="DetalleDevolucionPortBinding"
                contract="SoftWSDetalleDevolucion.DetalleDevolucion" name="DetalleDevolucionPort" />
			<endpoint address="http://localhost:8080/SoftBodWS/DetalleVenta"
                binding="basicHttpBinding" bindingConfiguration="DetalleVentaPortBinding"
                contract="SoftWSDetalleVenta.DetalleVenta" name="DetalleVentaPort" />
			<endpoint address="http://localhost:8080/SoftBodWS/Devolucion"
                binding="basicHttpBinding" bindingConfiguration="DevolucionPortBinding"
                contract="SoftWSDevolucion.Devolucion" name="DevolucionPort" />
			<endpoint address="http://localhost:8080/SoftBodWS/HistorialOperaciones"
                binding="basicHttpBinding" bindingConfiguration="HistorialOperacionesPortBinding"
                contract="SoftWSHistorialOperaciones.HistorialOperaciones"
                name="HistorialOperacionesPort" />
			<endpoint address="http://localhost:8080/SoftBodWS/RazonDevolucion"
                binding="basicHttpBinding" bindingConfiguration="RazonDevolucionPortBinding"
                contract="SoftWSRazonDevolucion.RazonDevolucion" name="RazonDevolucionPort" />
			<endpoint address="http://localhost:8080/SoftBodWS/RegistroPagoFiado"
                binding="basicHttpBinding" bindingConfiguration="RegistroPagoFiadoPortBinding"
                contract="SoftWSRegistroPagoFiado.RegistroPagoFiado" name="RegistroPagoFiadoPort" />
			<endpoint address="http://localhost:8080/SoftBodWS/VentaAlFiado"
                binding="basicHttpBinding" bindingConfiguration="VentaAlFiadoPortBinding"
                contract="SoftWSVentaAlFiado.VentaAlFiado" name="VentaAlFiadoPort" />
		</client>
	</system.serviceModel>
	<!--
    Para obtener una descripción de los cambios de web.config, vea http://go.microsoft.com/fwlink/?LinkId=235367.

    Los siguientes atributos se pueden establecer en la etiqueta <httpRuntime>.
      <system.Web>
        <httpRuntime targetFramework="4.8.1" />
      </system.Web>
  -->
	<system.web>
		<compilation debug="true" targetFramework="4.8.1"/>
		<httpRuntime targetFramework="4.7.2"/>
	</system.web>
</configuration>